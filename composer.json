{
  "name": "formassembly/qae-sample-project",
  "description": "FormAssembly Sample Project",
  "config": {
    "bin-dir": "bin",
    "sort-packages": true,
    "vendor-dir": "vendor",
    "discard-changes": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.2.0",
    "ext-dom": "*",
    "ext-gettext": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mcrypt": "*",
    "ext-simplexml": "*",
    "ext-soap": "*",
    "ext-xml": "*",
    "ext-xmlwriter": "*",
    "ext-zip": "*"
  },
  "require-dev": {
    "behat/behat": "3.3.1",
    "behat/mink": "1.7.1",
    "behat/mink-extension": "^2.2",
    "behat/mink-goutte-driver": "1.2.1",
    "behat/mink-selenium2-driver": "1.3.1",
    "sensiolabs/behat-page-object-extension": "^2.0"
  }
}
